#include <bits/stdc++.h>
using namespace std;
int main()
{
    double epsi = 1.0;

    int num = 0;
    while(true)
    {
        epsi = epsi / 10;
        num++;
        if(1+epsi>1)
        {
            continue;
        }
        else{
            break;
        }
    }
  
    epsi = epsi * 10;
    cout<<epsi<<endl<<num;
}
